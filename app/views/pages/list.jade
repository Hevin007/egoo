doctype html
html
	head
		meta(charset="utf-8")
		title #{title}
		link(href="/libs/bootstrap/dist/css/bootstrap.min.css",rel="stylesheet")
		script(src="/libs/jquery/dist/jquery.min.js")
		script(src="/libs/bootstrap/dist/js/bootstrap.min.js")
	body
		.container
			.row
				.page-header
					h1=title
				table.table.table.hover.table-bordered
					thead
						tr
							th 租户ID
							th 坐席ID
							th 用户ID
							th 会话ID
							th 业务类型
							th 业务分类
							th 来电号码
							th 客户姓名
							th 联系地址
							th 内容
							th 创建时间
						tbody
							each item in workSheets
								tr(class="item-id-#{item._id}")
									td #{item.tenantid}
									td #{item.agentid}
									td #{item.userid}
									td #{item.sessionid}
									td #{item.busType}
									td #{item.busGroup}
									td #{item.phone1}
									td #{item.clientName}
									td #{item.address}
									td #{item.content}
									td #{moment(item.meta.createaAt).format('MM/DD/YYYY')}